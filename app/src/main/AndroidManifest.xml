<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ashu.tlt">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/tour"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.HOME" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Splashscreen"
            android:theme="@style/MyTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContentDisplay"
            android:theme="@style/MyTheme_colorPrimaryDark"/>
        <activity
            android:name=".ContentDisplayTri"
            android:theme="@style/MyTheme_colorPrimaryDark"/>
        <activity
            android:name=".ContentDisplayNorth"
            android:theme="@style/MyTheme_colorPrimaryDark"/>
        <activity
            android:name=".ContentDisplayTiger"
            android:theme="@style/MyTheme_colorPrimaryDark"/>
        <activity
            android:name=".ContentDisplayPanther"
            android:theme="@style/MyTheme_colorPrimaryDark"/>
    </application>

</manifest>